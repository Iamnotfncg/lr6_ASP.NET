@model User

<h2>Registration</h2>
<form asp-action="Order" method="post">
    <div>
        <label asp-for="Name">Name:</label>
        <input asp-for="Name" />
    </div>
    <div>
        <label asp-for="Age">Age:</label>
        <input asp-for="Age" />
    </div>
    <div>
        <label asp-for="ProductQuantity">Quantity of Products:</label>
        <input asp-for="ProductQuantity" />
    </div>
    <button type="submit">Register</button>
</form>
<p>@ViewBag.Message</p>
